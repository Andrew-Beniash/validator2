{
  "name": "validator-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Monorepo containing frontend and backend packages",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "npm install",
    "start:dev": "npm run dev --workspaces --if-present",
    "start:frontend": "npm run dev --workspace=packages/frontend",
    "start:backend": "npm run dev --workspace=packages/backend",
    "build": "npm run build --workspaces --if-present",
    "test": "npm run test --workspaces --if-present",
    "lint": "npm run lint --workspaces --if-present"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "monorepo",
    "workspace",
    "react",
    "express"
  ],
  "author": "",
  "license": "ISC"
}
